<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HasilActivity"
    android:layout_margin="16dp">

    <TextView
        android:id="@+id/textHasil"
        android:textSize="20sp"
        android:gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="HASIL INPUT KONTAK"
        android:layout_margin="16dp"/>

    <TextView
        android:id="@+id/textNama"
        android:layout_below="@id/textHasil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama yang diinput : "/>

    <TextView
        android:id="@+id/hnama"
        android:layout_below="@id/textNama"
        android:textSize="20sp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/TextAlamat"
        android:layout_below="@id/hnama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alamat yang diinput : "/>

    <TextView
        android:id="@+id/halamat"
        android:layout_below="@id/TextAlamat"
        android:textSize="20sp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/textEmail"
        android:layout_below="@id/halamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email yang diinput : "/>

    <TextView
        android:id="@+id/hemail"
        android:layout_below="@id/textEmail"
        android:textSize="20sp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


    <TextView
        android:id="@+id/textTTL"
        android:layout_below="@id/hemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tanggal Lahir yang diinput : "/>


    <TextView
        android:id="@+id/hlahir"
        android:layout_below="@id/textTTL"
        android:textSize="20sp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>




    <TextView
        android:id="@+id/textTelp"
        android:layout_below="@id/hlahir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Telpon yang diinput : "/>

    <TextView
        android:id="@+id/htelpon"
        android:layout_below="@id/textTelp"
        android:textSize="20sp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>



</RelativeLayout>